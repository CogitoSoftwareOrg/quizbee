{
  "name": "quizbee-workspace",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "pnpm -r --filter ./pkgs/** run build",
    "dev": "pnpm -r --parallel --filter \"srvs/*\" run dev",
    "test": "pnpm -r --filter ./pkgs/** test",
    "lint": "pnpm -r lint",
    "version:changes": "changeset version",
    "publish:changes": "changeset publish",
    "release": "pnpm version:changes && pnpm -r build && pnpm publish:changes",
    "ci:build": "pnpm -r --filter \"@quizbee/*\" --if-present build",
    "ci:publish": "pnpm -r --filter \"@quizbee/*\" install && pnpm run -w ci:build && pnpm changeset publish"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c",
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "0.7.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild",
      "sharp"
    ]
  }
}
