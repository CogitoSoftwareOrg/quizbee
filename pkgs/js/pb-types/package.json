  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/quizbee.git",
    "directory": "pkgs/js/pb-types"
  },
  "keywords": [
    "pocketbase",
    "types",
    "typescript",
    "quizbee"
  ],
  "license": "MIT"
}
