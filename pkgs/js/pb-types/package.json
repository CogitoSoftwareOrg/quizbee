{
  "name": "@quizbee/pb-types",
  "version": "0.1.0",
  "description": "PocketBase TypeScript types for QuizBee",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "src"
  ],
  "sideEffects": false,
