---
import BlogPost from "@/components/pages/blog/BlogPost.astro";
import { getStaticBlogPostLocalePaths } from "@/ssg/blog";

export const getStaticPaths = getStaticBlogPostLocalePaths;

const { meta, content, article, toc, previousPost, nextPost } = Astro.props;
---

<BlogPost
  meta={meta}
  article={article}
  content={content}
  toc={toc}
  previousPost={previousPost}
  nextPost={nextPost}
/>
